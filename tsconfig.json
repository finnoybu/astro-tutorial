{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@posts/*": [
        "src/pages/posts/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ]
    }
  }
}